#include<stdio.h>
#include<limits.h>
int main(){
    int t;
    scanf("%d",&t);
    for(int i=0;i<t;i++){
        int n;
        scanf("%d",&n);
        char a[n];
        scanf("%s",a);
        int b[26]={0};
        for(int i=0;i<n;i++){
            b[a[i]-'a']++;
        }
        int max=INT_MIN,s;
        for(int i=0;i<n;i++){
            if(b[i]>max){
                max=b[i];
                s=i;
                
            }
        }
        printf("%c - %d",a[s],max);
        printf("\n");
    }
}
